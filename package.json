{
  "name": "voxel-physics-engine",
  "version": "0.7.2",
  "description": "Simple but physical engine for voxel games",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run install:typings && npm run build",
    "install:typings": "typings install",
    "lint": "tslint -c tslint.json \"src/**/*.+(ts|tsx)\"",
    "test": "npm run lint",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "build:watch": "npm run build -- --watch"
  },
  "author": "Andy Hall",
  "license": "MIT",
  "keywords": [
    "voxel",
    "voxeljs",
    "physics",
    "engine"
  ],
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "object-assign": "^4.1.0",
    "voxel-aabb-sweep": "^0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyhall/voxel-physics-engine"
  },
  "bugs": {
    "url": "https://github.com/andyhall/voxel-physics-engine/issues"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "typings": "^1.3.3"
  }
}
